{
  "name": "md-in-js",
  "description": "Material Design in JS",
  "version": "1.7.0",
  "author": "Dennis Torres <djtorres0@gmail.com>",
  "ava": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "bugs": {
    "url": "https://github.com/psirenny/md-in-js/issues"
  },
  "devDependencies": {
    "async": "2.5.0",
    "ava": "0.21.0",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.7.0",
    "fixpack": "2.3.1",
    "flow-bin": "0.49.1",
    "fs-extra": "3.0.1",
    "lodash": "4.17.4",
    "rollup": "0.45.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "3.0.0"
  },
  "homepage": "https://github.com/psirenny/md-in-js",
  "keywords": [
    "constants",
    "css",
    "design",
    "js",
    "material"
  ],
  "license": "Unlicense",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "repository": "https://github.com/psirenny/md-in-js",
  "scripts": {
    "ava": "ava",
    "build": "babel-node --presets=es2015,stage-0 build.js",
    "check": "npm run fixpack && npm run flow check && npm run lint && npm run test",
    "fixpack": "fixpack",
    "flow": "flow",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "npm run ava"
  }
}
